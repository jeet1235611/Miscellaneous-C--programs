#include <iostream>
using namespace std;

int main() {
        int a, b;
        cout << "Enter the value of a and b " << endl;
        cin >> a >> b;

        cout << "Before swapping: " << "a = "<< a << "\tb = " << b << endl;
        a = a + b;
        b = a - b;
        a = a - b;

        cout << "After swapping: " << "a = "<< a <<"\tb = " << b << endl;

        return 0;
}
